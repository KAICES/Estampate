<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>UsuarioServlet</servlet-name>
        <servlet-class>co.edu.uniminuto.arqSw.proyectoEstampas.servlets.UsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EstampaServlet</servlet-name>
        <servlet-class>co.edu.uniminuto.arqSw.proyectoEstampas.servlets.EstampaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VentasServlet</servlet-name>
        <servlet-class>co.edu.uniminuto.arqSw.proyectoEstampas.servlets.VentasServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EstampasPropioServlet</servlet-name>
        <servlet-class>co.edu.uniminuto.arqSw.proyectoEstampas.servlets.EstampasPropioServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UsuarioServlet</servlet-name>
        <url-pattern>/UsuarioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EstampaServlet</servlet-name>
        <url-pattern>/EstampaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VentasServlet</servlet-name>
        <url-pattern>/VentasServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EstampasPropioServlet</servlet-name>
        <url-pattern>/EstampasPropioServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
